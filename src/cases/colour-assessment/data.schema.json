  "$id": "https://id.uvci.eu/colour-assessment.schema.json",
  "title": "Input Data for risk colour assessment",
  "type": "object",
  "properties": {
      "countryOfInterest": {
        "type": "string",
        "pattern": "^[A-Z]{2}$"
